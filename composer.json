{
    "name": "gchernikov/error-bundle",
    "description": "Symfony bundle for handling errors and make responses",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "require": {
        "gchernikov/openapi-response-attributes": "*",
        "symfony/framework-bundle": "^5|^6",
        "symfony/serializer": "^5|^6",
        "symfony/validator": "^5|^6",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "GChernikov\\ErrorBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gennady Chernikov",
            "email": "chernikovg@gmail.com"
        }
    ],
    "repositories": {
        "support": {
            "type": "path",
            "url":  "../openapi-response-attributes"
        }
    }
}
